<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">
  
  <module name="docusign" package="com.axelor.apps.docusign.db"/>

  <entity name="DocuSignSigner" lang="java">

    <string name="name" title="Name" namecolumn="true"/>
    <many-to-one name="docuSignEnvelope" title="envelope" ref="com.axelor.apps.docusign.db.DocuSignEnvelope"/>
    <many-to-one name="docuSignSignerSetting" title="Signer setting" ref="com.axelor.apps.docusign.db.DocuSignSignerSetting"/>
    <many-to-one name="signer" title="Signer" ref="com.axelor.apps.base.db.Partner"/>
    <many-to-many name="signerSet" title="Signers" ref="com.axelor.apps.base.db.Partner"/>
    <many-to-one name="company" title="Company" ref="com.axelor.apps.base.db.Company"/>
    <one-to-many name="docuSignFieldList" title="Fields" ref="com.axelor.apps.docusign.db.DocuSignField" mappedBy="docuSignSigner"/>
    <integer name="sequence" title="Seq."/>
    <string name="accessCode" title="Access code"/>
    <boolean name="isGroup" title="Group"/>
    <boolean name="isInPersonSigner" title="In person signer"/>
  
  </entity>  
  
</domain-models>