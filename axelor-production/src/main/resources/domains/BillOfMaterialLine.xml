<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="BillOfMaterialLine" lang="java">

  	<integer name="priority" title="Priority"/>
  	<decimal name="qty" title="Qty" precision="20" scale="10"/>
  	<many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit" required="true"/>
  	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product" required="true"/>
  	<many-to-one name="parent" ref="com.axelor.apps.production.db.BillOfMaterial" title="Parent BOM" />
  	<many-to-one name="billOfMaterial" ref="com.axelor.apps.production.db.BillOfMaterial" title="Bill of Materials"/>
  </entity>
</domain-models>
