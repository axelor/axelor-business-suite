	<many-to-one name="analyticGrouping6" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 6"/>
	<many-to-one name="analyticGrouping7" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 7"/>
	<many-to-one name="analyticGrouping8" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 8"/>
	<many-to-one name="analyticGrouping9" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 9"/>
	<many-to-one name="analyticGrouping10" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 10"/>
  </entity>

</domain-models>
