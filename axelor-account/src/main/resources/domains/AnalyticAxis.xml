    <boolean name="manageParentAccount" title="Manage Parent Account" massUpdate="true"/>
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
    <many-to-one name="analyticGrouping1" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 1"/>
	<many-to-one name="analyticGrouping2" ref="com.axelor.apps.account.db.AnalyticGrouping" title="Analytic grouping 2"/>
