<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form id="account-company-form" name="company-form" title="Company" model="com.axelor.apps.base.db.Company" extension="true">
		<extend target="//field[@name='timezone']">
			<insert position="after">
				<field name="taxNumberList" colSpan="12"
					   canSelect="true" canNew="false" canEdit="false"
					   domain="self.company.id = :id"/>
			</insert>
		</extend>
	</form>

</object-views>