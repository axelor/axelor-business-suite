<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form id="account-fiscal-position-form" name="fiscal-position-form" title="Fiscal position" model="com.axelor.apps.account.db.FiscalPosition" extension="true">
		<extend target="//panel-related[@name='taxEquivListPanel']">
			<insert position="after">
				<panel-related name="taxNumberPanel" colSpan="12" field="taxNumberSet"
							   form-view="tax-number-form" grid-view="tax-number-grid"
							   canNew="false" canEdit="false" canSelect="true"/>
			</insert>
		</extend>
	</form>	
	
</object-views>
