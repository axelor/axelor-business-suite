<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid model="com.axelor.apps.base.db.PartnerSupplychainLink"
		title="Supply chain links" editable="true"
		name="partner-supplychain-link-grid">
		<field name="partner1"></field>
		<field name="partnerSupplychainLinkType"></field>
		<field name="partner2"></field>
	</grid>

	<grid model="com.axelor.apps.base.db.PartnerSupplychainLink"
		title="Supply chain links" editable="true"
		name="partner-supplychain-link-partner-1-grid">
		<field name="partnerSupplychainLinkType" target-name="link2Name"></field>
		<field name="partner2"></field>
	</grid>

	<grid model="com.axelor.apps.base.db.PartnerSupplychainLink"
		title="Supply chain links" editable="true"
		name="partner-supplychain-link-partner-2-grid">
		<field name="partner1"></field>
		<field name="partnerSupplychainLinkType" target-name="link1Name"></field>
	</grid>

	<form model="com.axelor.apps.base.db.PartnerSupplychainLink"
		title="Supply chain" name="partner-supplychain-link-form">
		<panel>
			<field name="partner1" colSpan="4"></field>
			<field name="partnerSupplychainLinkType" colSpan="4"></field>
			<field name="partner2" colSpan="4"></field>
		</panel>
	</form>

</object-views>
