<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.1.xsd">

  <module name="network" package="com.axelor.apps.network.db"/>

  <entity name="SnProfile" cachable="true">
    <string name="uniqueID" title="Unique ID"/>
    <integer name="snTypeSelect" selection="sn.profile.sn.type"/>    
    <many-to-one name="partner" title="Partner" ref="com.axelor.apps.base.db.Partner"/>
    <one-to-one name="snUserDetail" title="Details" ref="com.axelor.apps.network.db.SnUsersList" mappedBy="snProfile"/>
  </entity>
  
</domain-models>
