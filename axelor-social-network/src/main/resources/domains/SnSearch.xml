<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.1.xsd">

  <module name="network" package="com.axelor.apps.network.db"/>

  <entity name="SnSearch" cachable="true">
  	<string name="lastName" title="Last name"/>
  	<string name="firstName" title="First name"/>
  	<string name="email" title="Email"/>
  	<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner"/>
  	<one-to-many name="snProfile" ref="com.axelor.apps.network.db.SnUsersList" mappedBy="snSearch"/>
  </entity>
  
</domain-models>
