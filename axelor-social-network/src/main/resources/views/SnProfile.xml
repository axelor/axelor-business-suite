<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">

	<grid name="sn-profile-network-grid" title="SN Profile List" model="com.axelor.apps.network.db.SnProfile">
		<field name="uniqueID"/>
		<field name="snTypeSelect"/>
	</grid>

	<form name="sn-profile-form" title="SN Profile" model="com.axelor.apps.network.db.SnProfile">
		<panel title="Overview">
			<field name="uniqueID"/>
			<field name="snTypeSelect"/>
		</panel>
		<panel title="Details">
			<field name="snUserDetail" form-view="sn-user-profile-form"/>
		</panel>
		
	</form>


</object-views>
