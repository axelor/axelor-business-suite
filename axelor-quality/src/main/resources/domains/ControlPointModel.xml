<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="quality" package="com.axelor.apps.quality.db"/>

	<entity name="ControlPointModel" lang="java" cachable="true">
		<string name="name" title="Control point name" required="true"/>
		<integer name="priority" title="Priority"/>
		<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product" />
		<many-to-one name="team" ref="com.axelor.apps.quality.db.QualityTeam" title="Team" />
		<many-to-one name="responsible" ref="com.axelor.apps.hr.db.Employee" title="Responsible" />
		<integer name="controlType" title="Control type" selection="quality.control.points.control.type.select"/>
		<integer name="testType" title="Test type" selection="quality.control.points.test.type.select" />
		<decimal name="controlFrequency" title="Control Frequency"/>
		<string name="instructions" title="Instructions"/>
		<string name="notes" title="Notes"/>
		<string name="messageIfFailure" title="Message if failure"/>
		<many-to-one name="counter" title="counter" ref="QualityProcess"/>
		<track/>
	</entity>

</domain-models>