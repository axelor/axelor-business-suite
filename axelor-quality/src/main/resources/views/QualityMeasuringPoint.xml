<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="quality-measuring-point-grid" title="Measuring points" model="com.axelor.apps.quality.db.QualityMeasuringPoint">
		<field name="name"/>
		<field name="measuringNote" widget="ImageSelect" x-labels="false"/>
		<field name="compliant"/>
		<field name="notCompliant"/>
		<field name="observations"/>
	</grid>

	<form name="quality-measuring-point-form" title="Measuring points" model="com.axelor.apps.quality.db.QualityMeasuringPoint">
		<panel name="mainPanel">
			<field name="name"/>
			<field name="measuringNote" widget="ImageSelect" x-labels="false"/>
			<field name="coefficient"/>
			<field name="compliant" colSpan="3" widget="boolean-switch"/>
			<field name="notCompliant" colSpan="3" widget="boolean-switch"/>
			<field name="photosList" colSpan="12"/>
			<field name="observations" colSpan="12" widget="html"/>
		</panel>
	</form>

</object-views>
