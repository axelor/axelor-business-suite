<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">
  
  <module name="cash-management" package="com.axelor.apps.cash.management.db"/>
  
  <entity name="CashManagementRecap" lang="java">
	
	<many-to-one name="company" ref="com.axelor.apps.base.db.Company" required="true" title="Company" />
	<many-to-one name="bankDetails" ref="com.axelor.apps.base.db.BankDetails" title="Bank"/>
	<decimal name="startingBalance" title="Starting Balance" required="true"/>
	<decimal name="endingBalance" title="Ending Balance"/>
	<decimal name="currentBalance"/>
	<date name="fromDate" title="From"/>
	<date name="toDate" title="To"/>
	<one-to-many name="cashManagementRecapLineList" ref="com.axelor.apps.cash.management.db.CashManagementRecapLine" title="Details" mappedBy="cashManagementRecap"/>
	
  </entity>
  
</domain-models>