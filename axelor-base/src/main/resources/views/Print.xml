<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="print-grid" title="Prints" model="com.axelor.apps.base.db.Print">
    <field name="name"/>
    <field name="metaModel"/>
    <field name="language"/>
  </grid>

  <form name="print-form" title="Print" model="com.axelor.apps.base.db.Print">
    <panel>
      <field name="name"/>
      <field name="metaModel" grid-view="meta-model-grid" form-view="meta-model-form"/>
      <field name="printSettings" grid-view="printing-setting-grid" form-view="printing-setting-form"/>
      <field name="language" grid-view="language-grid" form-view="language-form"/>
      <field name="printLineList" grid-view="print-line-grid" form-view="print-line-form" colSpan="12"/>
      <field name="birtTemplateList" grid-view="birt-template-grid" form-view="birt-template-form" colSpan="12"/>
    </panel>
  </form>

</object-views>
