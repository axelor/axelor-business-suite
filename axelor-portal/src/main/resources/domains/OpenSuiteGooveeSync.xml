		<string name="osToGooveeFilter" title="Filter OS to goovee" />
		<string name="gooveeToOsFilter" title="Filter goovee to OS" />
		<one-to-many name="dynamicFieldSyncList" ref="com.axelor.apps.portal.db.DynamicFieldSync" title="Dynamic fields sync" mappedBy="openSuiteGooveeSync"/>

