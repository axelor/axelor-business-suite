<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
    
    <grid name="incoterm-grid" title="Incoterm grid" model="com.axelor.apps.stock.db.Incoterm">
    	<field name="name"/>
    	<field name="code"/>
    </grid>
    
    <form name="incoterm-form" title="Incoterm form" model="com.axelor.apps.stock.db.Incoterm">
    	<panel>
    		<field name="name" required="true" colSpan="12"/>
    		<field name="code" required="true" colSpan="12"/>
    	</panel>
    	<panel name="allocationsOfCosts" title="Allocations of Costs">
    		<field name="exportCustomsDeclaration"/>
			<field name="carriageToPortOfExport"/>
			<field name="unloadingOfTruckInPortOfExport"/>
			<field name="carriageToPortOfImport"/>
			<field name="insurance"/>
			<field name="unloadingInPortOfImport"/>
			<field name="loadingOnTruckInPortOfImport"/>
			<field name="carriageToPlaceOfDestination"/>
			<field name="importCustomsClearance"/>
			<field name="importDutiesAndTaxes"/>
    	</panel>
    </form>
    
 </object-views>