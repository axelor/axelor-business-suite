<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="auth" package="com.axelor.auth.db"/>

  <entity name="User" sequential="true" lang="java">
    <boolean name="useSignatureForStock" title="Use signature for certificate of conformity"/>
    
    <many-to-one name="activeStockLocation" ref="com.axelor.apps.stock.db.StockLocation" title="Active stock location"/>
    
    <many-to-one name="savedGroup" ref="com.axelor.auth.db.Group"/>
    <string name="savedHomeAction"/>
    
    
    
    <track>
	   <field name="useSignatureForStock"/>
    </track>
  </entity>

</domain-models>