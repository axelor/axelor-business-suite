<?xml version="1.0" encoding="utf-8"?>
<menu-builders xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  
  
  <menu-builder name="bpm-dashboard-root" title="BPM dashboard" icon="fa-pie-chart" background="red" />
  
  <menu-builder name="bpm-dashboard-book-dashboard" title="Book dashboard" parent="bpm-dashboard-root" show-action="true">
  	<action name="bpm.dashboard.book.dashboard" title="Book dashboard">
  		<views>
  			<view type="dashboard" name="book-dashboard" />
  		</views>
  	</action>
  </menu-builder>
  
  <menu-builder name="bpm-dashboard-product-dashboard" title="Product dashboard" parent="bpm-dashboard-root" show-action="true">
  	<action name="bpm.dashboard.product.dashboard" title="Product dashboard">
  		<views>
  			<view type="dashboard" name="product-order-dashboard" />
  		</views>
  	</action>
  </menu-builder>
  
  
</menu-builders>
