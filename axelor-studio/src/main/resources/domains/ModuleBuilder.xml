<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="studio" package="com.axelor.studio.db" />
  
	<entity name="ModuleBuilder" cachable="true">
	    <string name="name" title="Name" required="true"/>
	    <integer name="typeSelect" title="Type" selection="studio.module.builder.type.select"/>
	    <many-to-one name="importMetaFile" ref="com.axelor.meta.db.MetaFile" title="Import file" />
	    <many-to-one name="exportMetaFile" ref="com.axelor.meta.db.MetaFile" title="Export file" />
	    <many-to-one name="generatedFile" ref="com.axelor.meta.db.MetaFile" title="Generated file" />
	    <string name="fileTypeSelect" title="File type" selection="studio.module.builder.file.type.select" required="true"/>
	    
	    <extra-code><![CDATA[
	    	//TYPE SELECT
	    	public static final int TYPE_EXPORT = 1;
	    	public static final int TYPE_IMPORT = 2;
	    	
			// FILE TYPE SELECT
	    	public static final String FILE_TYPE_ZIP = "zip";
	    	public static final String FILE_TYPE_EXCEL = "excel";
	    	
	    ]]></extra-code>
	    
	</entity>

</domain-models>
