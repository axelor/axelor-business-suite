<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="studio" package="com.axelor.studio.db" />
  
	<entity name="ModuleBuilder" cachable="true">
	    <string name="name" title="Name" required="true"/>
	    <many-to-one name="metaFile" ref="com.axelor.meta.db.MetaFile" title="File" />
	    <string name="fileTypeSelect" title="Type" selection="studio.module.builder.file.type.select" required="true"/>
	    
	    <extra-code><![CDATA[
			    
	    	public static final String FILE_TYPE_ZIP = "zip";
	    	public static final String FILE_TYPE_EXCEL = "excel";
	    	
	    ]]></extra-code>
	    
	</entity>

</domain-models>
