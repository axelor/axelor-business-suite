<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <selection name="field.type.selection">
        <option value="integer">Integer</option>
        <option value="decimal">Decimal</option>
        <option value="string">String</option>
        <option value="boolean">Boolean</option>
        <option value="many-to-one">ManyToOne</option>
        <option value="one-to-many">OneToMany</option>
        <option value="many-to-many">ManyToMany</option>
        <option value="date">Date</option>
        <option value="datetime">DateTime</option>
    </selection>
    
    <selection name="studio.chart.builder.date.type">
        <option value="day">Day</option>
        <option value="month">Month</option>
        <option value="year">Year</option>
    </selection>
    
    <selection name="studio.chart.builder.display.type">
        <option value="0">Value</option>
        <option value="1">Percentage(%)</option>
    </selection>
	
    <selection name="studio.chart.builder.chart.type">
        <option value="bar">Bar</option>
        <option value="hbar">Horizontal bar</option>
        <option value="line">Line</option>
        <option value="donut">Donut</option>
        <option value="pie">Pie Chart</option>
        <option value="area">Area</option>
        <option value="text">Text</option>
        <option value="radar">Radar</option>
        <option value="gauge">Gauge</option>
        <option value="scatter">Scatter</option>
        <option value="funnel">Funnel</option>
    </selection>
    
    <selection name="studio.filter.operator">
        <option value="=">equals</option>
        <option value="!=">not equal</option>
        <option value="&gt;=">greater or equal</option>
        <option value="&lt;=">less or equal</option>
        <option value="&gt;">greater than</option>
        <option value="&lt;">less than</option>
        <option value="like">contains</option>
        <option value="notLike">doesn't contains</option>
        <option value="isNull">is null</option>
        <option value="notNull">is not null</option>
		<option value="isTrue">is true</option>
		<option value="isFalse">is false</option>       
		<option value="include">include</option>
		<option value="notInclude">not include</option> 
    </selection>
    
    <selection name="studio.business.wkf.display.type.select">
        <option value="0">Nav Select</option>
        <option value="1">Simple Select</option>
    </selection>
    
    <selection name="studio.chart.filter.logic.operator">
    	<option value="0">AND</option>
    	<option value="1">OR</option>
    </selection>
    
    <selection name="studio.condition.alert.type.select">
    	<option value="1">Blocking</option>
    	<option value="2">Alert</option>
    </selection>
    
    <selection name="studio.view.calculation.line.type.select">
    	<option value="0">Arithmetic</option>
    	<option value="1">Groovy</option>
    </selection>
    
    <selection name="studio.action.builder.type.select">
    	<option value="0">Create</option>
    	<option value="1">Update</option>
    	<option value="2">Script</option>
    </selection>
    
    <selection name="studio.action.builder.line.type.select">
    	<option value="0">Formula</option>
    	<option value="1">Groovy</option>
    </selection>
    
    <selection name="studio.action.builder.line.validation.type.select">
    	<option value="error">Error</option>
    	<option value="alert">Alert</option>
    	<option value="info">Information</option>
    	<option value="notify">Notification</option>
    </selection>
    
    <selection name="app.type.select" id="studio.app.type.select">
		<option order="21" value="bpm">Bpm</option>
	</selection>
	
	<selection name="studio.action.builder.script.type.select">
		<option value="0">Javascript</option>
    	<option value="1">Groovy</option>
	</selection>
    
</object-views>