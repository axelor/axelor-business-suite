<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

		<module name="event" package="com.axelor.apps.event.db" />

		<entity name="EventAddress" lang="java">
				<string name="flatHouseNo" required="true" title="Flat/House No." />
				<string name="street" required="true" title="Street" />
				<string name="landMark" required="true" title="LandMark" />
				<string name="city" required="true" title="City" />
				<string name="country" required="true" title="Country" />
				<string name="fullName" namecolumn="true"
						search="flatHouseNo,street,landMark,city,country" readonly="true" title="Full Address"> 
        <![CDATA[
        
          		 return flatHouseNo+ " " + street + " " + landMark + " " + city + " " + country ;
       
        ]]>
				</string>

		</entity>

</domain-models>