<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="contract" package="com.axelor.apps.contract.db" />

	<entity name="Alert" lang="java" cachable="true">
		<date name="alertDate" title="Date" />
		<string name="resason" title="Reason"  />
		<integer name="reminderSelect" title="Reminder" selection="contract.alert.reminder.select"  required="true"/>
		<string name="reminderTypeSelect" title="Reminder type" selection="contract.alert.reminder.type.select" required="true" default="days"/>
	</entity>

</domain-models>