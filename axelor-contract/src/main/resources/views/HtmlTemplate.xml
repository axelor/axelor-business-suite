<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
    
    <grid name="html-template-grid" title="Contract template" model="com.axelor.apps.contract.db.HtmlTemplate">
       <field name="name" />
    </grid>
    
    <form name="html-template-form" title="Contract template" model="com.axelor.apps.contract.db.HtmlTemplate">
       <panel>
	       <field name="name" />
	       <field name="content" colSpan="12" widget="html" />
       </panel>
    </form>

</object-views>